<script>
	var resizeTimer;

    $(window).resize(function(){
	    clearTimeout(resizeTimer);
		resizeTimer = setTimeout(function(){
			smartSize()
	    }, 500)
    })

    $('.slider-blocks, .slide_wrapper').on('mousedown touchstart', function (e) {
        //e.preventDefault();
        xDown = e.pageX || e.originalEvent.touches[0].pageX;
    }).on('mouseup touchend',function (e) {
        xUp = e.pageX || e.originalEvent.changedTouches[0].pageX;
        if(xDown != xUp && diff(xDown, xUp) > 50){
            if(xDown > xUp){
                swipe('left')
            }else{
                swipe('right')
            }
        }
    });

    function diff(a,b){return Math.abs(a-b);}

    function swipe(way){
	     if($(window).width() <= 768){
		     var active_element = $('.slider-dots').find('.active').index();
		     var total_elements = $('.slider-dots > div').length;

		     if(way == 'left'){
			     var new_element = active_element+1;
			     if(new_element < total_elements){
				     $('.slider-dots > div').eq(new_element).trigger('click');
			     }
		     }else{
			     var new_element = active_element-1;
			     if(new_element >= 0){
				     $('.slider-dots > div').eq(new_element).trigger('click');
			     }
		     }

	     }
    }

    function smartSize(){
	    if($(window).width() <= 768){
		    var maxHeight = 0;
		    $('.items > li > div > p').each(function() {
			    maxHeight = maxHeight > $(this).outerHeight(true) ? maxHeight : $(this).outerHeight(true);
				});
				$('.items > li > div > p').stop(true,true).outerHeight(maxHeight);
	    }else{
		    $('.slider-blocks, .slide_wrapper').find('.items').css({ transform: 'translate3d(0px, 0px, 0px)'})
		    $('.items > li > div > p').stop(true,true).height('auto');
	    }
    }

    $('.slider-dots > div').bind({
        click: function(){
	        $('.slider-dots > div').removeClass('active');
        	var ind = $(this).index();
        	var div = $('.slider-blocks, .slide_wrapper');
        	$(this).addClass('active');
        	setSmartSlide(ind, div);
        }
    })

	function setSmartSlide(ind, div){
		$(div).data('cur', ind);
		smartSlide()
	}

    function smartSlide(){
		var pos = $('.slider-blocks, .slide_wrapper').data('cur');
		var slide_width = $('.slider-blocks, .slide_wrapper').find('.items').outerWidth(true)-50;
		var x = (-pos * slide_width);
		$('.slider-blocks, .slide_wrapper').find('.items').css({ transform: 'translate3d('+x+'px, 0px, 0px)'});
	}

	smartSize();
</script>



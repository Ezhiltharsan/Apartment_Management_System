    $('.animation-header').click(function(e){
        let category = $(e.target).attr('data-testimonial');
        $('.animation-header').removeClass('active');
        $('.animation-headers').find('[data-testimonial="'+category+'"]').addClass('active');
        $('.video-block').removeClass('active');
        $('.testimonials-wrapper').find('[data-testimonial="'+category+'"]').addClass('active');
    });

    $('.mute-btn').click(function(e) {
        let video_block = $(e.target).closest('.video-block');
        let vid = video_block.find('video');
        vid.get()[0].muted = false;
        $(e.target).hide();
        video_block.find('.unmute-btn').show();
    });
    $('.unmute-btn').click(function(e) {
        let video_block = $(e.target).closest('.video-block');
        let vid = video_block.find('video');
        vid.get()[0].muted = true;
        $(e.target).hide();
        video_block.find('.mute-btn').show();
    });
    $('.play-btn').click(function(e) {
        let video_block = $(e.target).closest('.video-block');
        let vid = video_block.find('video');
        vid.get()[0].play();
        $(e.target).hide();
    });
</script>

